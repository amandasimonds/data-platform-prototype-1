{
  "name": "uv-ui-lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve uv-ui-prototype",
    "start-ut": "ng serve uv-ui-prototype --configuration local-ut",
    "build": "ng build uv-ui-prototype",
    "deploy-gh": "ng build uv-ui-prototype --configuration production && cp -r ./dist/uv-ui-prototype ../docs",
    "build-dev": "ng build uv-ui-prototype --configuration dev",
    "test": "ng test",
    "lint": "eslint -c .eslintrc.json --ext .ts projects/uv-ui-prototype",
    "lint-lib": "eslint -c .eslintrc.json --ext .ts projects/uv-ui-lib",
    "lint-scss": "npx stylelint **/*.scss",
    "build-prototype": "ng build uv-ui-prototype -s ../uv-ui-lib/src/lib/assets",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s ./projects/uv-ui-lib/src/lib/assets",
    "build-storybook-watch": "npm run docs:json && build-storybook -w -s -s ./projects/uv-ui-lib/src/lib/assets",
    "build-storybook": "npm run docs:json && build-storybook -s ./projects/uv-ui-lib/src/lib/assets",
    "build-uv-ui-lib": "ng build uv-ui-lib --configuration production",
    "build-publish-lib": "ng build uv-ui-lib --configuration production && cd ./dist/uv-ui-lib/ && npm publish",
    "pack-uv-ui-lib": "ng build uv-ui-lib --configuration production && cd ./dist/uv-ui-lib/ && npm pack",
    "build-uv-ui-lib:watch": "ng build uv-ui-lib --watch",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@material/slider": "^13.0.0",
    "date-fns": "^2.23.0",
    "echarts": "^5.2.2",
    "lodash": "^4.17.21",
    "ngx-echarts": "^8.0.0",
    "rxjs": "^6.6.7",
    "xng-breadcrumb": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "^13.5.0",
    "@angular-eslint/eslint-plugin": "^13.5.0",
    "@angular-eslint/eslint-plugin-template": "^13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "^13.5.0",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.5.0-alpha.64",
    "@storybook/addon-essentials": "^6.5.0-alpha.64",
    "@storybook/addon-links": "^6.5.0-alpha.64",
    "@storybook/angular": "^6.5.0-alpha.64",
    "@types/echarts": "^4.9.12",
    "@types/jasmine": "^3.6.11",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.20.13",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.25.0",
    "@typescript-eslint/parser": "5.3.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "angular-cli-ghpages": "1.0.6",
    "codelyzer": "^6.0.2",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-unicorn": "^28.0.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^15.2.2",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^21.0.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0"
  }
}
