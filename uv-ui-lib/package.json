{
  "name": "uv-ui-lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve uv-ui-prototype",
    "start-ut": "ng serve uv-ui-prototype --configuration local-ut",
    "build": "ng build uv-ui-prototype",
    "build-dev": "ng build uv-ui-prototype --configuration dev",
    "deploy-dev": "npm run build-dev && azcopy copy dist/uv-ui-prototype/* https://uvuiprototype.blob.core.windows.net/$web --recursive",
    "build-aws": "ng build uv-ui-prototype --configuration aws-ut",
    "deploy-aws": "npm run build-aws && aws s3 cp dist/uv-ui-prototype s3://uv-design-prototype-usability-testing --recursive",
    "build-azure-ut": "ng build uv-ui-prototype --configuration azure-ut",
    "deploy-azure-ut": "npm run build-azure-ut && azcopy copy dist/uv-ui-prototype/* https://uvuiusabilitytest.blob.core.windows.net/$web --recursive",
    "deploy-azure": "npm run deploy-dev && npm run deploy-azure-ut",
    "deploy-all": "npm run deploy-dev && npm run deploy-aws && npm run deploy-azure-ut",
    "test": "ng test",
    "lint": "eslint -c .eslintrc.json --ext .ts projects/uv-ui-prototype",
    "lint-lib": "eslint -c .eslintrc.json --ext .ts projects/uv-ui-lib",
    "lint-scss": "npx stylelint **/*.scss",
    "build-prototype": "ng build uv-ui-prototype -s ../uv-ui-lib/src/lib/assets",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s ./projects/uv-ui-lib/src/lib/assets",
    "build-storybook-watch": "npm run docs:json && build-storybook -w -s -s ./projects/uv-ui-lib/src/lib/assets",
    "build-storybook": "npm run docs:json && build-storybook -s ./projects/uv-ui-lib/src/lib/assets",
    "build-uv-ui-lib": "ng build uv-ui-lib --configuration production",
    "build-publish-lib": "ng build uv-ui-lib --configuration production && cd ./dist/uv-ui-lib/ && npm publish",
    "pack-uv-ui-lib": "ng build uv-ui-lib --configuration production && cd ./dist/uv-ui-lib/ && npm pack",
    "build-uv-ui-lib:watch": "ng build uv-ui-lib --watch",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.12",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.12",
    "@angular/compiler": "^13.3.12",
    "@angular/core": "^13.3.12",
    "@angular/forms": "^13.3.12",
    "@angular/material": "^12.2.12",
    "@angular/platform-browser": "^13.3.12",
    "@angular/platform-browser-dynamic": "^13.3.12",
    "@angular/router": "^13.3.12",
    "@antv/g6": "^4.8.3",
    "@material/slider": "^13.0.0",
    "angular-auth-oidc-client": "^12.0.1",
    "date-fns": "^2.23.0",
    "echarts": "^5.2.2",
    "epd-pattern-library": "^1.1.40",
    "lodash": "^4.17.21",
    "ng-antv-g6": "^0.0.2",
    "ngx-echarts": "^8.0.0",
    "rxjs": "^6.6.7",
    "xng-breadcrumb": "^7.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.10",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "^13.3.10",
    "@angular/compiler-cli": "^13.3.12",
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.5.0-alpha.64",
    "@storybook/addon-essentials": "^6.5.0-alpha.64",
    "@storybook/addon-links": "^6.5.0-alpha.64",
    "@storybook/angular": "^6.5.0-alpha.64",
    "@types/echarts": "^4.9.12",
    "@types/jasmine": "^3.6.11",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.20.13",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.25.0",
    "@typescript-eslint/parser": "4.28.2",
    "@webcomponents/custom-elements": "^1.4.3",
    "codelyzer": "^6.0.2",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-unicorn": "^28.0.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^21.0.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0"
  }
}
